programa
{
	
	funcao inicio()
	{
		inteiro num1

		escreva("Digite um número inteiro: ")
		
		leia(num1)

		escreva("\n ",num1)

		enquanto(num1 <= 100){
			
			num1 = num1*3
			escreva(" \n ", num1)			
			
		}


		
	}
}
